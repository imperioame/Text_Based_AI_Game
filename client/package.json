{
  "name": "text-adventure-ai-client",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "autoprefixer": "^10.4.14",
    "axios": "^1.4.0",
    "iconv-lite": "^0.6.3",
    "lodash": "^4.17.21",
    "lucide-react": "^0.439.0",
    "postcss": "^8.4.24",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.0"
  },
  "scripts": {
    "tailwind": "npx tailwindcss -i ./src/tailwind.css -o ./src/tailwind.output.css",
    "sass": "sass src/styles.scss src/styles.css",
    "start": "npm run tailwind && npm run sass && react-scripts start",
    "build": "cross-env REACT_APP_API_URL=https://ai-text-game.marioa.me npm run tailwind && npm run sass && react-scripts build",
    "build:dev": "cross-env REACT_APP_API_URL=https://ai-text-game.marioa.me npm run tailwind && npm run sass && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "react-scripts": "5.0.1",
    "sass": "^1.78.0",
    "tailwindcss": "^3.3.2"
  }
